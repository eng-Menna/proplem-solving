#include <iostream>
using namespace std;
int main()
{ios_base::sync_with_stdio(false);
    int n,m,z,counter=0;
    cin>>n>>m>>z;
    for (int i=n ;i<=z;i+=n)/*3mlt l increment kda 34an he said n increases n , 2n w hakza*/
        {for(int  j=m;j<=z;j+=m)
        {if(i==j)
          counter++;}}

        cout<< counter <<endl;

    return 0;
}
